{
  "name": "react-timer-machine",
  "version": "1.0.2",
  "description": "React Timer Machine is a fully controllable and customizable timer component for react",
  "homepage": "https://yassinedoghri.github.io/react-timer-machine/",
  "license": "MIT",
  "repository": {
    "type": "git",
